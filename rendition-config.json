{
    "inputConfigs": [
        {
            "path" : "\\raw-assets"
            , "fileType": "jpg"
        }
    ]
    , "outputConfig": {
        "dirPath": "\\renders"
        , "deepPath": "\\[image-name]"
        , "renderResolutions": [
            {
                "aspectRatio" : "16/9"
                , "dimensions" : ["1920.1080", "1366.768", "890.500"]
            }
            , {
                "aspectRatio" : "4/3"
                , "dimensions" : ["1024.768", "512.384"]
            }
            , {
                "aspectRatio" : "3/4"
                , "dimensions" : ["768.1024", "384.512"]
            }
            , {
                "aspectRatio" : "9/16"
                , "dimensions" : ["500.890", "768.1366", "1920.1080"]
            }
            , {
                "aspectRatio" : "9/20"
                , "dimensions" : ["360.800"]
            }
        ]
    }
}